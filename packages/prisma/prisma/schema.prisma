// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tag {
  id       Int    @id @default(autoincrement())
  name     String @unique
  content  String
  authorId BigInt
}

model Guild {
  id BigInt @id
}
